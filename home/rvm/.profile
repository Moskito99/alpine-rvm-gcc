
export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*
export rvm_bin_path=/home/rvm/.rvm/bin
export GEM_HOME=/home/rvm/.rvm/gems/ruby-2.3.0
export IRBRC=/home/rvm/.rvm/rubies/ruby-2.3.0/.irbrc
export MY_RUBY_HOME=/home/rvm/.rvm/rubies/ruby-2.3.0
export rvm_path=/home/rvm/.rvm
export rvm_prefix=/home/rvm
export PATH=/home/rvm/.rvm/gems/ruby-2.3.0/bin:/home/rvm/.rvm/gems/:ruby-2.3.0@global/bin:/home/rvm/.rvm/rubies/ruby-2.3.0/bin:/home/rvm/.rvm/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
export rvm_version=1.26.11
export GEM_PATH=/home/rvm/.rvm/gems/ruby-2.3.0:/home/rvm/.rvm/gems/ruby-2.3.0@global
export RUBY_VERSION=ruby-2.3.0

